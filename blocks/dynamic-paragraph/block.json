{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "interactive-fiction-engine/dynamic-paragraph",
	"version": "0.1.0",
	"title": "Dynamic Paragraph",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"displayConditionally": {
			"type": "boolean",
			"default": false
		},
		"conditionTarget": {
			"type": "string",
			"default": ""
		},
		"conditionValue": {
			"type": "integer",
			"default": 0
		},
		"target": {
			"type": "string",
			"default": ""
		},
		"content": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "interactive-fiction-engine",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
