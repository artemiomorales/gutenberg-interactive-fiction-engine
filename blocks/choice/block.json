{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "interactive-fiction-engine/choice",
	"version": "0.1.1",
	"title": "Interactive Fiction Engine - Choice",
	"category": "widgets",
	"icon": "smiley",
	"description": "Create rich content and conditionally hide or display other Choice blocks.",
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"parentId": {
			"type": "string",
			"default": ""
		},
		"condition": {
			"type": "integer",
			"default": null
		},
		"choices": {
			"type": "array",
			"default": []
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "interactive-fiction-engine",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
