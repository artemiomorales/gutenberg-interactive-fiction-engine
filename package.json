{
	"name": "interactive-fiction-engine",
	"version": "0.1.0",
	"description": "Blocks and scripts to support the creation of interactive fiction using Gutenberg.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start:choice": "wp-scripts start blocks/choice/src/index.js --output-path=blocks/choice/build/",
		"build:choice": "wp-scripts build blocks/choice/src/index.js --output-path=blocks/choice/build/",
		"start:js": "wp-scripts start js/src/index.js --output-path=js/build/",
		"build:js": "wp-scripts build js/src/index.js --output-path=js/build/"
	},
	"devDependencies": {
		"@wordpress/scripts": "^24.3.0"
	}
}
